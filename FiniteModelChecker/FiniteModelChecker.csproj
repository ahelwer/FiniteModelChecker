<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0</Version>
    <Description>A finite model checking framework for C# code</Description>
    <Authors>Andrew Helwer</Authors>
    <Company>Microsoft</Company>
    <PackageLicenseUrl>https://github.com/ahelwer/FiniteModelChecker/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/ahelwer/FiniteModelChecker</PackageProjectUrl>
    <RepositoryUrl>https://github.com/ahelwer/FiniteModelChecker.git</RepositoryUrl>
    <PackageTags>z3;test;testing</PackageTags>
    <IncludeSymbols>true</IncludeSymbols>
    <IncludeSource>true</IncludeSource>
  </PropertyGroup>

</Project>
